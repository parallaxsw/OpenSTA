library(simple) {
  time_unit : 1ns;
  voltage_unit : 1V;
  current_unit : 1mA;
  pulling_resistance_unit : 1kohm;
  
  default_operating_conditions : typical;
  
  operating_conditions(typical) {
    process : 1.0;
    voltage : 1.8;
    temperature : 25.0;
  }
  
  cell(NAND2) {
    area : 4.0;
    
    pin(A) {
      direction : input;
      capacitance : 0.02;
    }
    
    pin(B) {
      direction : input;
      capacitance : 0.02;
    }
    
    pin(Z) {
      direction : output;
      function : "(A & B)'";
      
      internal_power() {
        related_pin : "A";
        when : "!B";
        rise_power(scalar) {
          values : 0.01;
        }
        fall_power(scalar) {
          values : 0.01;
        }
      }
      
      internal_power() {
        related_pin : "B";
        when : "!A";
        rise_power(scalar) {
          values : 0.01;
        }
        fall_power(scalar) {
          values : 0.01;
        }
      }
      
      timing() {
        related_pin : "A";
        when : "!B";
        timing_type : combinational;
        cell_rise(scalar) {
          values : 0.05;
        }
        cell_fall(scalar) {
          values : 0.05;
        }
        rise_transition(scalar) {
          values : 0.02;
        }
        fall_transition(scalar) {
          values : 0.02;
        }
      }
    }
    
    leakage_power() {
      value : 0.001;
    }
  }
}
